{
  "name": "Salesbot",
  "description": "Salesbot is an open-source SALESbot",
  "repository": "https://github.com/tcob/salesbot",
  "success_url": "/",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "SALESBOT_COMMAND_TOKEN": {
      "description": "Slash command token, for the Salesbot command endpoint",
      "required": true
    },
    "SLACK_TOKEN": {
      "description": "Slack bot RTM API token",
      "required": false
    },
    "NODE_ENV": "staging"
  },
  "image": "heroku/nodejs"
}
